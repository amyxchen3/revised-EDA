        "  xlab(\"Major Category\")+",
        "+ ",
        "  ylab(\"Number of Graduates\")",
        "> ",
        "ggplot(recent_grads, aes(x=Major_category, y= Median, fill=ShareWomen))+",
        "+ ",
        "  geom_bar(stat=\"identity\")+",
        "+ ",
        "  coord_flip()+",
        "+ ",
        "  labs(x= \"Major Category\", y = \"Median Salary\", fill=\"Proportion of Women\")"
    ]
}